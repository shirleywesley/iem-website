
@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    @*@Umbraco.RenderMacro("MetaTitle")
    @Umbraco.RenderMacro("Meta")*@

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    @RenderSection("HeaderScripts", required:false)
    @*@Html.Raw(DeveloperSettings.HeaderScripts)*@
  </head>
  <body @*@Html.Raw(ViewBag.AdditionalAttributes)>*@

    @RenderBody()

    @RenderSection("FooterScripts", required: false)
    @*@Html.Raw(DeveloperSettings.FooterScripts)*@
  </body>
</html>